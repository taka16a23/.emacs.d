# -*- mode: snippet -*-
# name: SvCtrl
# key: svctrl
# expand-env: ((yas/indent-line 'fixed) (yas/wrap-around-region 'nil))
# --
try
{

    /* サーバーURLセット */
    $1.SvCtrl.SetServerURL("/gyoumu/$10/$20");

    /* パラメーターセット */
    $1.SvCtrl.AddParam("$30", $40);
    
    /* 通信 */
    $1.SvCtrl.connect();

    $0
}
catch ( exp )
{
    debugMessage( "例外" + exp.message + "が" + exp.Method + "で発生しました" );
    $50
}
