# -*- mode: snippet -*-
# name: eval-after-load mode map
# key: (eval-after-load
# condition: (not (at-string|comment-p))
# --
(defun $1-mode-predefine ()
  "For \`eval-after-load' $1 function."
  (message "Loading $1..")
  ($1-mode-map-predefine))

(defun $1-mode-map-predefine ()
 "For $1 key bindings."
 (interactive)
 ($0))

(eval-after-load '${1:filename} '($1-mode-predefine)