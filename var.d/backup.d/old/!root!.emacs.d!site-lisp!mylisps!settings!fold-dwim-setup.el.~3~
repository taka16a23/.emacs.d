(eval-when-compile
  (require 'bind-key)
  (require 'use-package "use-package" 'noerr))

(declare-function package-bundle "package-setup.el")
(when (require 'package-setup "package-setup" 'noerr)
  (ignore-errors (package-bundle 'fold-dwim)))

(use-package fold-dwim
  ;; :disabled
  :defer
  :commands
  (fold-dwim-toggle fold-dwim--hide-all fold-dwim-show-all)
  :init
  (require 'bind-key)
  :config
  (message "Loading \"fold-dwim\"")
  :bind
  (("M-i" . fold-dwim-toggle)
   ("C-x M-i" . fold-dwim-hide-all)
   ("C-x M-I" . fold-dwim-show-all)
   ))



(provide 'fold-dwim-setup)
;; For Emacs
;; Local Variables:
;; coding: utf-8
;; End:
;;; fold-dwim-setup.el ends here
